[MASTER]
ignore=migrations, venv, __pycache__
load-plugins=pylint_django

[MESSAGES CONTROL]
disable=
    missing-docstring,
    too-few-public-methods,
    trailing-newlines,
    missing-final-newline

[FORMAT]
max-line-length=120

[TYPECHECK]
ignored-classes=options

[DESIGN]
max-parents=7
max-attributes=12

[LOGGING]
logging-modules=logging,django.utils.log